{
  "name": "mock-responses",
  "version": "2.0.2",
  "description": "espress-kind http request middleware",
  "main": "dist/main.js",
  "bin": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allenhwkim/mock-responses.git"
  },
  "keywords": [
    "express",
    "intercept",
    "middleware"
  ],
  "author": "Allen Kim <allenhwkim@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/allenhwkim/mock-responses/issues"
  },
  "homepage": "https://github.com/allenhwkim/mock-responses#readme",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "npm run start:copy-assets && ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "start:exmaple": "nodemon demo/express.js",
    "start:copy-assets": "sh copy-assets.sh",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/platform-express": "^6.7.2",
    "better-sqlite3": "4.1.4",
    "ejs": "^2.6.1",
    "glob": "^7.1.3",
    "hbs": "^4.0.5",
    "http-proxy": "^1.16.2",
    "http-proxy-middleware": "^0.20.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "username": "^4.0.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@nestjs/cli": "^6.8.4",
    "@nestjs/schematics": "^6.6.6",
    "@nestjs/testing": "^6.7.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/supertest": "^2.0.8",
    "browser-sync": "^2.26.5",
    "cryptiles": "^4.1.2",
    "express": "^4.16.2",
    "html-custom-elements": "^0.5.2",
    "install": "^0.13.0",
    "jest": "^24.9.0",
    "mime": "^1.4.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.3",
    "normalize.css": "^8.0.1",
    "npm": "^6.11.3",
    "opn": "^5.3.0",
    "prettier": "^1.18.2",
    "randomatic": "^3.0.0",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsc-watch": "^4.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "webpack-dev-server": "^3.1.11"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  }
}
